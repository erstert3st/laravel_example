              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "notice"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "fixedColor": "green",
                    "mode": "fixed"
                  }
                }
              ]
            }
          ]
