              "type": "loki",
              "uid": "P8E80F9AEF21F6940"
            },
            "direction": "backward",
            "editorMode": "code",
            "expr": "sum(count_over_time({job=~\"$job\", logtype=\"error\", level!=\"\"}[$__interval])) by (level)",
            "legendFormat": "{{ level }}",
            "queryType": "range",
            "refId": "A"
          }
        ],
        "title": "Logs by level",
        "type": "timeseries"
      },
