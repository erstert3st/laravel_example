            "datasource": {
              "type": "prometheus",
              "uid": "${prometheus_datasource}"
            },
            "editorMode": "code",
            "expr": "sum by (le,job) (increase(apache_response_http_codes_bucket{le!=\"+Inf\", job=~\"$job\"}[$__rate_interval]))\n",
            "format": "heatmap",
            "legendFormat": "",
            "range": true,
            "refId": "A"
          }
        ],
        "title": "Logs by HTTP codes",
        "transformations": [
          {
            "disabled": true,
            "id": "renameByRegex",
            "options": {
              "regex": "(\\d).+",
              "renamePattern": "HTTP $100-$199"
            }
          }
        ],
        "type": "timeseries"
      },
